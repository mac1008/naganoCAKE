<h1>会員一覧</h1>

<% @customers.each do |customer| %>
<table>
  <tr>
    <th>会員ID</th>
    <th>氏名</th>
    <th>メールアドレス</th>
    <th>ステータス</th>
  </tr>
  <tr>
    <td><%= customer.id %></td>
    <td><%= customer.name %></td><!--クリックできるようにし、遷移先はadmin_customer_path(customer.id-->
    <td><%= customer.email %></td>
    <td><%= customer.is_active %></td>
  </tr>
</table>

<%= paginate customers %>